{
  "registry": {
    "inputs": {
      "nixpkgs": {
        "from": {
          "type": "github"
        , "owner": "NixOS"
        , "repo": "nixpkgs"
        , "rev":  "e8039594435c68eb4f780f3e9bf3972a7399c4b1"
        }
      , "subtrees": ["legacyPackages"]
      }
    , "floco": {
        "from": {
          "type": "github"
        , "owner": "aakropotkin"
        , "repo": "floco"
        }
      , "subtrees": ["packages"]
      }
    , "nixpkgs-flox": {
        "from": {
          "type": "github"
        , "owner": "flox"
        , "repo": "nixpkgs-flox"
        }
      , "subtrees": ["catalog"]
      , "stabilities": ["stable", "staging", "unstable"]
      }
    }
  , "priority": ["nixpkgs", "floco", "nixpkgs-flox"]
  }
, "systems": ["x86_64-linux"]
, "query":   {
    "pname": "hello"
  , "semver": ">=2"
  }
}
